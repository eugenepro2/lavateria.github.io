extends include/layout.pug

block content
	section.slider
		.swiper-container
			.swiper-wrapper
				.swiper-slide(style="background-image: url('img/slider/2.jpg')")
					.container
						.row
							.col-xs-12
								h1.wow.fadeIn Стираем, <br>
									i.icon-slider-wave
									| сушим, гладим,<br>доставляем!
				.swiper-slide(style="background-image: url('img/slider/3.jpg')")
					.container
						.row
							.col-xs-12
								h1.wow.fadeIn Стираем, <br>
									i.icon-slider-wave
									| сушим, гладим,<br>доставляем!
				.swiper-slide(style="background-image: url('img/slider/4.jpg')")
					.container
						.row
							.col-xs-12
								h1.wow.fadeIn Стираем, <br>
									i.icon-slider-wave
									| сушим, гладим,<br>доставляем!
			.swiper-pagination
		.container
			.offer
				.row
					.col-xs-12
						.offer__block.wow.fadeIn#offer
							h2 Я хочу cдать в стирку
							.row
								.col-xs-12.offer__block__row
									.row
										.col-sm-7
											h3 Одежда<br>и постельное белье
												.tooltip
													.tooltip__hover i am tooltip
											span Рубашки, футболки, юбки, брюки, наволочки и т.д.
										.col-sm-5
											.flex.between-xs.middle-xs.offer__block__calc.calc
												.add(data-action="substract")
													span
													i -
												input(type="number" data-action="keyup" data-price="80" value="0" min="0" max="99" onkeypress="return isNumeric(event)" oninput="maxLengthCheck(this)" placeholder="0")
												a(href="#offer").add(data-action="add")
													span
													i +
								.col-xs-12.offer__block__row
									.row
										.col-sm-7
											h3 Крупные вещи 
												.tooltip
													.tooltip__hover i am tooltip i am tooltipi am tooltipi am tooltip
											span Пуховики, куртки, одеяла,<br> пледы и т.д.
										.col-sm-5
											.flex.between-xs.middle-xs.offer__block__calc.calc
												.add(data-action="substract")
													span
													i -
												input(type="number" data-action="keyup" data-price="100" value="0" min="0" max="99" onkeypress="return isNumeric(event)" oninput="maxLengthCheck(this)" placeholder="0")
												a(href="#offer").add(data-action="add")
													span
													i +
								.col-xs-12.offer__block__delivery
									.row
										.col-sm-12
											h4 Доставка:
										.col-sm-6.offer__block__delivery__checkbox
											.checkbox
												.checkbox__block
												input#checkbox-delivery-1(type="radio" name="delivery" data-delivery-price="300" data-delivery-price-free="2000")
												label.label(for="checkbox-delivery-1")
												label.label-for(for="checkbox-delivery-1") В пределах КАД
										.col-sm-6.offer__block__delivery__checkbox
											.checkbox
												.checkbox__block
												input#checkbox-delivery-2(type="radio" name="delivery" data-delivery-price="500" data-delivery-price-free="3000")
												label.label(for="checkbox-delivery-2")
												label.label-for(for="checkbox-delivery-2") До 30 км от КАД
										.col-sm-12.offer__block__delivery__checkbox
											.checkbox
												.checkbox__block
												input#checkbox-delivery-3(type="radio" name="delivery" data-delivery-price="0" data-delivery-price-free="0")
												label.label(for="checkbox-delivery-3")
												label.label-for(for="checkbox-delivery-3") Самовывоз: ул. Рентгена, д. 9 литера Б
								.col-xs-12
									.offer__block__free Стирка нижнего белья 
										span бесплатно
						.offer__cart
							.offer__cart__reset
								img(src="img/close.svg", alt="")
							h2 В корзине: 
							.row
								.col-xs-12(data-price="80")
									.row.middle-xs.offer__cart__row
										.col-sm-2
											.offer__cart__count.cart__count
												span 0
												.offer__cart__count__span вещей
										.col-sm-4
											span.offer__cart__onsum на сумму:
										.col-sm-6
											.offer__cart__sum.cart__sum.cart__sum--1
												h3 
													span 0
													i.icon-ruble
												.offer__cart__sum__span 80 р. за вещь
								.col-xs-12(data-price="100")
									.row.offer__cart__row.middle-xs
										.col-sm-2
											.offer__cart__count.cart__count
												span 0
												.offer__cart__count__span вещей
										.col-sm-4
											span.offer__cart__onsum на сумму:
										.col-sm-6
											.offer__cart__sum.cart__sum.cart__sum--2
												h3 
													span 0
													i.icon-ruble
												.offer__cart__sum__span 100 р. за вещь
								.col-xs-12
									.row.offer__cart__delivery.middle-xs
										.col-sm-6
											h2 Доставка
										.col-sm-6
											.offer__cart__delivery__sum#cart-delivery
												h3 
													span#cart-delivery-sum 0 
													i.icon-ruble
												span#cart-delivery-sum-free от <span></span> р. - бесплатно
								.col-xs-12
									hr
								.col-xs-12
									.row.middle-xs
										.col-sm-4
											.offer__cart__total.total Итого:<br>
												span 0
												i.icon-ruble
										.offer__cart__btn
											a.btn.btn--slider(href="checkout.html") 
												i Заказать
	section.services
		.container
			.row
				.col-xs-12
					.heading.wow.fadeInDown(data-wow-delay="0.3s")
						h2 Наши услуги
						i.icon-wave-heading
				.col-sm-6.end-sm.wow.fadeIn(data-wow-delay="0.3s")
					a.services__block.flex.bottom-xs.center-xs(style="background-image: url('img/services/1.png')" href="#")
						.services__block__block
							h3 Стирка<br>одежды с глажкой
							.btn.btn--services
								i Подробнее
							.services__block__hover
				.col-sm-6.wow.fadeIn(data-wow-delay="0.4s")
					a.services__block.flex.top-xs.center-xs(style="background-image: url('img/services/2.png')" href="#")
						.services__block__block
							h3 Стирка<br>одеял, подушек, пледов
							.btn.btn--services
								i Подробнее
							.services__block__hover
				.col-sm-6.end-sm.wow.fadeIn(data-wow-delay="0.3s")
					a.services__block.flex.bottom-xs.center-xs(style="background-image: url('img/services/3.png')" href="#")
						.services__block__block
							h3 Аквачистка<br>курток, пуховиков
							.btn.btn--services
								i Подробнее
							.services__block__hover
				.col-sm-6.wow.fadeIn(data-wow-delay="0.4s")
					a.services__block.flex.top-xs.center-xs(style="background-image: url('img/services/4.png')" href="#")
						.services__block__block
							h3 Профессиональная<br>чистка обуви
							.btn.btn--services
								i Подробнее
							.services__block__hover
	section.subscription
		.container
			.row
				.col-xs-12
					.heading.wow.fadeInDown(data-wow-delay="0.3s")
						h2 Абонементное обслуживание  						
						i.icon-wave-heading
			.row.subscription__row
				.col-sm-5
					.subscription__block.subscription__block--blue.wow.fadeIn(data-wow-delay="0.3s")
						.subscription__block__header.flex.middle-xs.center-xs
							h2 Абонемент<br> по корзинам белья
						.subscription__block__content
							.row
								.col-sm-6
									h4 Количество корзин<br>в месяц:
									select.blue.select-first
										option(data-ratio="1" data-value="1" data-price="500") 1 корзина
										option(data-ratio="0.95" data-value="2" data-price="500") 2 корзины
								.col-sm-6
									h4 Время действия абонемента:
									select.blue.select-second
										option(data-ratio-month="1" data-value-month="1") 1 месяц
										option(data-ratio-month="0.95" data-value-month="2") 2 месяца
							.row
								.col-xs-12
									ul
										li Наша корзина для белья вместимостью 40 литров позволяет загружать 2 комплекта постельного белья, пуховик, джинсы, рубашки и многое другое;
										li При каждом посещении наш курьер привезет Вам чистую корзину для белья и заберет заполненную грязным бельем;
										li Дополнительно к корзине белья Вы можете сдать неограниченное количество нижнего белья.
							.row.subscription__block__total.middle-xs
								.col-sm-5
									h3 
										span 0
										i.icon-ruble
									span 500 <i class="icon-ruble"></i>  / корзина 
								a.btn.btn--subscription--blue(href="#") 
									i Оформить абонемент
				.col-sm-7.end-sm
					.subscription__basket
						.subscription__basket__block
							h2 Вы заполнили корзину под “завязку”? 
							p Сообщите нам и курьер заберет грязное белье и привезет новую корзину для заполнения
						img.subscription__basket__arrow(src="img/arrow-big-left.svg", alt="")
						img.subscription__basket__basket(src="img/subscription-basket.png", alt="")
			.row.subscription__row
				.col-sm-7.end-sm
					.subscription__clothes
						.subscription__clothes__block
							h2 Вы собрали вещи<br>для стирки? 
							p Сообщите нам и курьер заберет грязное белье и вернет Вам чистое в течении 24 часов
						img.subscription__clothes__arrow(src="img/arrow-big-right.svg", alt="")
						img.subscription__clothes__clothes(src="img/subscription-clothes.png", alt="")
				.col-sm-5
					.subscription__block.subscription__block--green.wow.fadeIn(data-wow-delay="0.3s")
						.subscription__block__header.flex.middle-xs.center-xs
							h2 Абонемент<br>по количеству вещей
						.subscription__block__content
							.row
								.col-sm-6
									h4 Количество вещей<br>в месяц:
									select.green.select-first
										option(data-ratio="1" data-value="1" data-price="2000") 20 вещей
										option(data-ratio="1" data-value="1" data-price="3000") 30 вещей
								.col-sm-6
									h4 Время действия абонемента:
									select.green.select-second
										option(data-ratio-month="1" data-value-month="1") 1 месяц
										option(data-ratio-month="0.95" data-value-month="2") 2 месяца
							.row
								.col-xs-12
									ul
										li Нижнее белье <span>беплатно</span> при каждом заборе;
										li Крупная вещь приравнивается<br> к 3-м обычным;
										li 4 приемки грязного и 4 доставки чистого;
							.row.subscription__block__total.middle-xs
								.col-sm-5
									h3 
										span 0
										i.icon-ruble
									span.subscription__block__total__span 100 <i class="icon-ruble"></i>  / вещь 
								.col-sm-7
									a.btn.btn--subscription--green(href="#") 
										i Оформить абонемент
